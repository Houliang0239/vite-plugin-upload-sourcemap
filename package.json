{
  "name": "vite-plugin-upload-sourcemap",
  "version": "v0.0.1-alpha",
  "description": "A vite plugin for upload sourcemap",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run build --watch --ignore-watch examples",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "vite:build": "tsup src/index.ts vite build",
    "example:react": "cd examples/vite-react && pnpm run build",
    "example:vue2": "cd examples/vite-vue2 && pnpm run build",
    "example:vue3": "cd examples/vite-vue3 && npm run build",
    "example:fund": "cd examples/yxnn-fundcourse-fe && npm run build:prod"
  },
  "keywords": [
    "vite",
    "plugin"
  ],
  "author": "Houliang0239",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2",
    "vite": "^2.8.4"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "archiver": "^5.3.1",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.0"
  }
}
